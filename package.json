{
  "name": "vns-cms",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "format": "cross-env NODE_OPTIONS=--no-deprecation pretty-quick --ignore-path .lintignore",
    "format:fix": "cross-env NODE_OPTIONS=--no-deprecation prettier --write . --ignore-path .lintignore",
    "tsc": "cross-env NODE_OPTIONS=--no-deprecation tsc -p tsconfig.json --noEmit",
    "clean": "node scripts/clean.mjs",
    "postinstall": "pnpm rebuild sharp",
    "stylelint": "stylelint --reportInvalidScopeDisables --reportNeedlessDisables \"src/**/*.?(c|m)[jt]s?(x)\" --ignore-path .gitignore"
  },
  "dependencies": {
    "@ai-stack/payloadcms": "3.2.16-beta",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@iconify/react": "^6.0.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/lab": "7.0.0-beta.16",
    "@mui/material": "^7.3.1",
    "@mui/material-nextjs": "^7.3.0",
    "@mui/utils": "^7.3.1",
    "@payloadcms/db-mongodb": "3.52.0",
    "@payloadcms/next": "3.52.0",
    "@payloadcms/payload-cloud": "3.52.0",
    "@payloadcms/plugin-cloud-storage": "^3.52.0",
    "@payloadcms/plugin-redirects": "^3.52.0",
    "@payloadcms/plugin-search": "^3.52.0",
    "@payloadcms/plugin-seo": "^3.52.0",
    "@payloadcms/richtext-lexical": "3.52.0",
    "@payloadcms/ui": "3.52.0",
    "@piplup/rhf-adapters": "^4.0.2",
    "@piplup/rhf-core": "^4.0.2",
    "@piplup/utils": "^3.2.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "culori": "^4.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "16.4.7",
    "es-toolkit": "^1.39.10",
    "graphql": "^16.11.0",
    "jump.js": "^1.0.2",
    "material-ui-confirm": "^4.0.0",
    "material-ui-popup-state": "^5.3.6",
    "motion": "^12.23.12",
    "next": "15.4.4",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "payload": "3.52.0",
    "photoswipe": "^5.4.4",
    "react": "~19.1.1",
    "react-dom": "~19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "reading-time": "^1.5.0",
    "schema-dts": "^1.1.5",
    "serialize-javascript": "^6.0.2",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "vaul": "^1.1.2",
    "warning": "^4.0.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@bprogress/next": "^3.2.12",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@next/eslint-plugin-next": "^15.5.0",
    "@piplup/code-infra": "^1.0.1",
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/culori": "^4.0.0",
    "@types/jump.js": "^1.0.6",
    "@types/node": "^22.17.2",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/serialize-javascript": "^5.0.4",
    "@types/warning": "^3.0.3",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "husky": "^9.1.7",
    "jsdom": "26.1.0",
    "lint-staged": "^16.1.5",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "postcss-custom-media": "^11.0.6",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.0.1",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "ignoredBuiltDependencies": [
      "@piplup/code-infra",
      "@tailwindcss/oxide"
    ]
  }
}
